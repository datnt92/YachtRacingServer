<?xml version="1.0" encoding="utf-8" ?>
<Extension>
    <Name>HorseRaceServer</Name>
    <ManagedObjects>
        <ManagedObject>
            <Handle>DatabaseControllerFactory</Handle>
            <Type>Java</Type>
            <Path>com.netgame.database.DatabaseControllerFactory</Path>
        </ManagedObject>
    </ManagedObjects>
    <EventHandlers>
    	<ExtensionLifecycleHandlers>
            <ExtensionLifecycleHandler>
                <Handle>GMSInitializer</Handle>
                <Type>Java</Type>
                <Path>com.netgame.gamemanager.GMSInitializer</Path>
            </ExtensionLifecycleHandler>
        </ExtensionLifecycleHandlers>
		<LoginHandlers>
			<LoginHandler>
				<Handle>LoginEventHandler</Handle>
				<Type>Java</Type>
				<Path>com.netgame.lobby.loginLogoutHandlers.LoginEventHandler</Path>
			</LoginHandler>
		</LoginHandlers>
		<LogoutHandlers>
			<LogoutHandler>
				<Handle>LogoutEventHandler</Handle>
				<Type>Java</Type>
				<Path>com.netgame.lobby.loginLogoutHandlers.LogoutEventHandler</Path>
			</LogoutHandler>
		</LogoutHandlers>
    </EventHandlers>
    <Plugins>
    	<Plugin>
            <Handle>LobbyPlugin</Handle>
            <Type>Java</Type>
            <Path>com.netgame.lobby.LobbyPlugin</Path>
        </Plugin>    
    </Plugins>
</Extension>

